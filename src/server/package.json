{
  "name": "cc-reactor-server",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "build": "tsc",
    "dockerize": "docker build . -t node-cc-reactor-server",
    "docker": "docker run -it --init -v cc-server-scripts:/usr/src/app/scripts -p 443:3000 --name cc-reactor-server node-cc-reactor-server",
    "restart-docker": "sudo docker stop cc-reactor-server && sudo docker rm cc-reactor-server && sudo npm run dockerize && sudo npm run docker",
    "start": "node build/app.js"
  },
  "author": "Ved",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "rate-limiter-flexible": "^2.2.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/node": "^15.12.1",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.3.0"
  },
  "type": "module"
}
